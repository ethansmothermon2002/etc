" -*- vimrc -*-
" vim: set filetype=vim:
" https://github.com/tridactyl/tridactyl/blob/master/.tridactylrc
unbind <C-y>
bind <C-S-r> source ~/.config/tridactyl/tridactylrc
bind <C-]> tabnext
bind <C-[> tabprev
bind <C-l> tabnext
bind <C-h> tabprev
" unbind <C-j>
" unbind <C-k>
" bind --mode=input <C-j> <Tab>
" bind --mode=input <C-k> <S-Tab>
bind D composite tabclose | buffer #

set searchurls.!clj https://clojuredocs.org/search?q=
set searchurls.!aw https://wiki.archlinux.org/index.php?title=Special:Search&search=
set searchurls.!mdn https://developer.mozilla.org/en-US/search?q=
set searchurls.!img https://www.google.com/search?tbm=isch&q=

set hintfiltermode vimperator-reflow
set hintnames numeric

set colorscheme srcery
