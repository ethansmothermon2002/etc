unbind <C-y>
bind <C-R> source ~/.config/tridactyl/tridactylrc
bind <C-]> tabnext
bind <C-[> tabprev
bind <C-l> tabnext
bind <C-h> tabprev
" bind --mode=insert <C-j> <Tab>
" bind --mode=insert <C-k> <S-Tab>
bind D composite tabclose | buffer #

set searchurls.clj https://clojuredocs.org/search?q=
set searchurls.aw https://wiki.archlinux.org/index.php?title=Special:Search&search=
set searchurls.mdn https://developer.mozilla.org/en-US/search?q=

set hintfiltermode vimperator-reflow
set hintnames numeric

set colorscheme dark

" vim: set filetype=vim:
