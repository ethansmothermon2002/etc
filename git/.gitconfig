[user]
	email = mail@roosta.sh
	name = Daniel Berg
[core]
	editor = vim
	excludesfile = ~/.gitignore
[push]
	default = simple
[credential]
	helper = cache
[alias]
  logs = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  logd = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -p
	logl = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	alias = "!git config -l | grep alias | cut -c 7-"
	graph = log --graph --oneline --decorate
	unstage = reset HEAD --
	last = log -1 HEAD
  rmc = rm --cached
